#test
fn test_unary_inv() {
    assert ~i256(1) == -2
    assert ~i128(1) == -2
    assert ~i64(4000000000) == -4000000001
    assert ~i32(30000) == -30001
    assert ~i16(2000) == -2001
    assert ~i8(1) == -2
    assert ~u256(1) == 115792089237316195423570985008687907853269984665640564039457584007913129639934
    assert ~u128(1) == 340282366920938463463374607431768211454
    assert ~u64(1) == 18446744073709551614
    assert ~u32(1) == 4294967294
    assert ~u16(1) == 65534
    assert ~u8(1) == 254
}